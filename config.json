{
  "mcpProxy": {
    "baseURL": "https://pro-kingfish-constantly.ngrok-free.app",
    "addr": ":9000",
    "name": "MCP Proxy",
    "version": "1.0.0",
    "options": {
      "logEnabled": true,
      "authTokens": []
    }
  },
  "mcpServers": {
    "coingecko": {
      "command": "python",
      "args": [
        "concurrent_mcp_server.py",
        "--mode",
        "public"
      ],
      "env": {
        "MCP_SERVER_NAME": "coingecko",
        "MCP_TRANSPORT": "sse"
      },
      "options": {
        "logEnabled": true,
        "toolFilter": {
          "mode": "allow",
          "list": [
            "coingecko__ping",
            "coingecko__get_price",
            "coingecko__get_token_price",
            "coingecko__get_supported_currencies",
            "coingecko__get_coins_list",
            "coingecko__get_coins_markets",
            "coingecko__get_coin_details",
            "coingecko__get_coin_tickers",
            "coingecko__get_coin_history",
            "coingecko__get_coin_market_chart",
            "coingecko__get_coin_market_chart_range",
            "coingecko__get_coin_ohlc",
            "coingecko__get_trending",
            "coingecko__get_global_data",
            "coingecko__get_global_defi",
            "coingecko__search",
            "coingecko__get_exchanges",
            "coingecko__get_exchange_details",
            "coingecko__get_categories",
            "coingecko__get_categories_list",
            "coingecko__get_exchange_rates",
            "coingecko__get_asset_platforms",
            "coingecko__get_companies_holdings"
          ]
        }
      }
    },
    "opensubtitles": {
      "command": "python",
      "args": [
        "concurrent_mcp_server.py",
        "--mode",
        "public"
      ],
      "env": {
        "MCP_SERVER_NAME": "opensubtitles",
        "MCP_TRANSPORT": "sse"
      },
      "options": {
        "logEnabled": true,
        "toolFilter": {
          "mode": "allow",
          "list": [
            "opensubtitles__search_subtitles",
            "opensubtitles__get_languages",
            "opensubtitles__download_subtitle"
          ]
        }
      }
    }
  }
}